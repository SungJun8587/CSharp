				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {\r",
									"    let jsonData = pm.response.json();\r",
									"    \r",
									"    if (jsonData.token.accessToken) {\r",
									"        pm.environment.set(\"accessToken\", jsonData.token.accessToken);\r",
									"    }\r",
									"\r",
									"    if (jsonData.token.refreshToken) {\r",
									"        pm.environment.set(\"refreshToken\", jsonData.token.refreshToken);\r",
									"    }\r",
									"} else {\r",
									"    console.log(\"Error response:\", pm.response.text());\r",
									"}"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
