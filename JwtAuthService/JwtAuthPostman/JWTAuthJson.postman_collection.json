				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) \r",
									"{\r",
									"    let jsonData = pm.response.json();\r",
									"    let accessToken = jsonData.token.accessToken;\r",
									"    let refreshToken = jsonData.token.refreshToken;\r",
									"\r",
									"    if (accessToken) \r",
									"    {\r",
									"        pm.environment.set(\"accessToken\", accessToken);\r",
									"    }\r",
									"\r",
									"    if (refreshToken)\r",
									"    {\r",
									"        pm.environment.set(\"refreshToken\", refreshToken);\r",
									"    }\r",
									"} \r",
									"else\r",
									"{\r",
									"    console.log(\"Error response:\", pm.response.text());\r",
									"}"
